<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>findAllClients</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>findAllProducts</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>findAllOrders</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>findOneOrders</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>findOneProducts</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>findOneClients</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>findAllClients</servlet-name>
        <url-pattern>/findAllClients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findAllProducts</servlet-name>
        <url-pattern>/findAllProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findAllOrders</servlet-name>
        <url-pattern>/findAllOrders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findOneOrders</servlet-name>
        <url-pattern>/findOneOrders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findOneProducts</servlet-name>
        <url-pattern>/findOneProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findOneClients</servlet-name>
        <url-pattern>/findOneClients</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
